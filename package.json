{
  "name": "@nuvix/storage",
  "version": "1.0.1",
  "description": "S3-compatible storage library for Nuvix BaaS platform with support for AWS S3, Wasabi, MinIO, and local storage",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": " ./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run clean && bun run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "xml2js": "^0.6.2",
    "eslint": "^8.38.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "tsup": "^8.5.0",
    "typescript": "^5.0.4"
  },
  "keywords": [
    "nuvix",
    "storage",
    "s3",
    "baas",
    "wasabi",
    "minio",
    "cloud-storage",
    "file-upload",
    "typescript",
    "esm"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nuvix-tech/storage.git"
  },
  "bugs": {
    "url": "https://github.com/nuvix-tech/storage/issues"
  },
  "homepage": "https://github.com/nuvix-tech/storage#readme"
}
